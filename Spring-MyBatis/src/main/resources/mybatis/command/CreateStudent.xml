<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.springmybatis.mapper.commands.ICreateStudentMapper">
    <insert id="insertStudent" parameterType="CreateStudentRequest" >
        INSERT INTO STUDENT (ID, NAME, ADDRESS, AGE, MALE)
        VALUES (#{id}, #{name}, #{address}, #{age}, #{male});
    </insert>
</mapper>